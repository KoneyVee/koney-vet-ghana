# Force npm to use the correct platform
platform=linuxmusl
# Set the Node.js version
node-version=18.x
# Ensure optional dependencies are installed
oppositional_http2_handshake=true
# Set the Python version if needed
python=python3

engine-strict=true
save-exact=true
audit=true
fund=false
loglevel=error
omit=optional
legacy-peer-deps=true
